{
  "name": "saltyrtc-client",
  "version": "0.1.5",
  "description": "SaltyRTC JavaScript implementation",
  "scripts": {
    "test": "karma start --single-run --log-level=debug --colors",
    "dist": "rollup -c rollup.dist.js && cp saltyrtc/saltyrtc.d.ts dist/",
    "rollup_tests": "rollup -c rollup.testing.js",
    "validate": "tsc --noEmit",
    "clean": "rm -rf saltyrtc/*.js tests/testsuite.js*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saltyrtc/saltyrtc-client-js.git"
  },
  "keywords": [
    "saltyrtc",
    "webrtc",
    "ortc",
    "rtc",
    "nacl"
  ],
  "author": "Threema GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saltyrtc/saltyrtc-client-js/issues"
  },
  "homepage": "https://github.com/saltyrtc/saltyrtc-client-js#readme",
  "devDependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "jasmine-core": "^2.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "tweetnacl": "^0.14.3"
  }
}
